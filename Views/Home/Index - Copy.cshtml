@section Scripts{
    <script src="/js/site/scripts/speech-service.js"></script>
    <script>
    //WelcomeNote();
    function WelcomeNote(){
        $.ajax({
            url:"/js/site/data/speech-welcome.json",
            success:function(data,response){
                speechService.speak(data.speechText);
            },
            error:function(xhr,e,m){
                console.log(xhr);
            }

        })
    }
    </script>
    
}
